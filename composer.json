{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "bigbluebutton/bigbluebutton-api-php": "~2.0.0",
        "chumper/zipper": "^1.0",
        "doctrine/dbal": "^2.10",
        "elibyy/tcpdf-laravel": "^5.8",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.2",
        "google/apiclient": "^2.4",
        "guzzlehttp/guzzle": "~6.0",
        "hisorange/browser-detect": "^3.1",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "lcobucci/jwt": "3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "ncjoes/office-converter": "^1.0",
        "nwidart/laravel-modules": "^6.2",
        "org_heigl/ghostscript": "^2.3",
        "pusher/pusher-php-server": "^4.1",
        "spatie/laravel-cors": "^1.3",
        "spatie/laravel-permission": "^2.37",
        "spatie/pdf-to-image": "^2.1",
        "unisharp/laravel-ckeditor": "^4.7"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "minimum-stability":"dev",
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
